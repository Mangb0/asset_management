{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { reactive } from \"vue\";\nimport axios from \"axios\";\nexport default {\n  setup() {\n    const state = reactive({\n      data: []\n    });\n    const pagelink = () => {\n      this.$router.push('/login');\n    };\n    const add = () => {\n      const content = prompt(\"내용을 입력해주세요.\");\n      if (!content) {\n        alert(\"내용을 입력해주세요.\");\n        return add();\n      }\n      axios.post(\"/api/assets\", {\n        content\n      }).then(res => {\n        state.data = res.data;\n      });\n    };\n    const edit = changeno => {\n      const content = prompt(\"내용을 입력해주세요\", state.data.find(d => d.changeno === changeno).money);\n      axios.put(\"/api/assets/\" + changeno, {\n        content\n      }).then(res => {\n        state.data = res.data;\n      });\n    };\n    axios.get(\"/api/assets\").then(res => {\n      state.data = res.data;\n    });\n    return {\n      state,\n      add,\n      edit,\n      pagelink\n    };\n  }\n};","map":{"version":3,"mappings":";AAcA,SAASA,QAAO,QAAS,KAAK;AAC9B,OAAOC,KAAI,MAAO,OAAO;AACzB,eAAe;EACXC,KAAK,GAAG;IACJ,MAAMC,KAAI,GAAIH,QAAQ,CAAC;MACnBI,IAAI,EAAE;IACV,CAAC,CAAC;IAEF,MAAMC,QAAO,GAAI,MAAM;MACnB,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,QAAQ,CAAC;IAC/B;IAEA,MAAMC,GAAE,GAAI,MAAM;MACd,MAAMC,OAAM,GAAIC,MAAM,CAAC,aAAa,CAAC;MACrC,IAAG,CAACD,OAAO,EAAE;QACTE,KAAK,CAAC,aAAa,CAAC;QACpB,OAAOH,GAAG,EAAE;MAChB;MACAP,KAAK,CAACW,IAAI,CAAC,aAAa,EAAE;QAAEH;MAAQ,CAAC,CAAC,CAACI,IAAI,CAAEC,GAAG,IAAK;QACjDX,KAAK,CAACC,IAAG,GAAIU,GAAG,CAACV,IAAI;MACzB,CAAC,CAAC;IACN,CAAC;IAED,MAAMW,IAAG,GAAKC,QAAQ,IAAK;MACvB,MAAMP,OAAM,GAAIC,MAAM,CAAC,YAAY,EAAEP,KAAK,CAACC,IAAI,CAACa,IAAI,CAACC,CAAC,IAAEA,CAAC,CAACF,QAAO,KAAMA,QAAQ,CAAC,CAACG,KAAK,CAAC;MACvFlB,KAAK,CAACmB,GAAG,CAAC,cAAa,GAAIJ,QAAQ,EAAE;QAAEP;MAAQ,CAAC,CAAC,CAACI,IAAI,CAAEC,GAAG,IAAK;QAC5DX,KAAK,CAACC,IAAG,GAAIU,GAAG,CAACV,IAAI;MACzB,CAAC,CAAC;IACN,CAAC;IAEDH,KAAK,CAACoB,GAAG,CAAC,aAAa,CAAC,CAACR,IAAI,CAAEC,GAAG,IAAK;MACnCX,KAAK,